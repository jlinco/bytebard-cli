{
  "name": "@bytebard/cli",
  "version": "1.0.0",
  "description": "Testing out our cli-platform",
  "keywords": [
    "react",
    "cli",
    "components",
    "ui",
    "react-native",
    "expo",
    "typescript",
    "scaffolding",
    "tanstack-react-query",
    "tanstack-router"
  ],
  "author": "Joel Cofie<bytebard><https://github.com/jlinco>",
  "license": "MIT",
  "type": "module",
  "exports": "./dist/index.js",
  "bin": {
    "cli-platform": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "build": "tsup",
    "start:dev": "node dist/index.js",
    "check:lint": "biome lint ./src",
    "check:format": "biome format ./src",
    "fix:lint": "biome lint --write ./src",
    "fix:format": "biome format --write ./src"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "@types/fs-extra": "^11.0.4",
    "@types/gradient-string": "^1.1.6",
    "@types/node": "^20.12.7",
    "@types/prompts": "^2.4.9",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "gradient-string": "^2.0.2",
    "ora": "^8.0.1",
    "type-fest": "^4.18.2",
    "zod": "^3.22.5"
  }
}
